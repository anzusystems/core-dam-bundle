{
  "name": "anzusystems/core-dam-bundle",
  "description": "Dam core bundle",
  "type": "symfony-bundle",
  "license": "Apache-2.0",
  "authors": [
  ],
  "prefer-stable": true,
  "require": {
    "php": ">=8.3",
    "ext-json": "*",
    "ext-mongodb": "*",
    "ext-pcntl": "*",
    "ext-redis": "*",
    "ext-simplexml": "*",
    "ext-vips": "*",
    "ext-xmlreader": "*",
    "ext-zend-opcache": "*",
    "anzusystems/common-bundle": "8.10",
    "doctrine/doctrine-bundle": "^2.12",
    "doctrine/orm": "^3.1",
    "elasticsearch/elasticsearch": "^8.6",
    "google/apiclient": "^2.12",
    "google/cloud-pubsub": "^1.39",
    "html2text/html2text": "^4.3",
    "jcupitt/vips": "^1.0",
    "league/flysystem": "^3.7",
    "league/flysystem-google-cloud-storage": "^3.7",
    "petitpress/gps-messenger-bundle": "^2.0",
    "php-ffmpeg/php-ffmpeg": "^1.0",
    "phpexiftool/exiftool": "^10.16",
    "symfony/dependency-injection": "^7.2",
    "symfony/doctrine-bridge": "^7.2",
    "symfony/dotenv": "^7.2",
    "symfony/expression-language": "^7.2",
    "symfony/framework-bundle": "^7.2",
    "symfony/http-client": "^7.2",
    "symfony/http-kernel": "^7.2",
    "symfony/lock": "^7.2",
    "symfony/messenger": "^7.2",
    "symfony/mime": "^7.2",
    "symfony/options-resolver": "^7.2",
    "symfony/polyfill-uuid": "^1.24",
    "symfony/process": "^7.2",
    "symfony/property-access": "^7.2",
    "symfony/runtime": "^7.2",
    "symfony/security-bundle": "^7.2",
    "symfony/uid": "^7.2",
    "symfony/validator": "^7.2",
    "symfony/yaml": "^7.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "psalm/plugin-symfony": "^5.0",
    "rector/rector": "^1.0",
    "slevomat/coding-standard": "^7.0",
    "symfony/test-pack": "^1.0",
    "symplify/easy-coding-standard": "^12.1",
    "vimeo/psalm": "^5.6",
    "weirdan/doctrine-psalm-plugin": "^2.8"
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php81": "*",
    "symfony/polyfill-php82": "*"
  },
  "autoload": {
    "psr-4": {
      "AnzuSystems\\CoreDamBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AnzuSystems\\CoreDamBundle\\Tests\\": "tests",
      "AnzuSystems\\CoreDamBundle\\Tests\\Data\\": "tests/data"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "symfony/runtime": true,
      "php-http/discovery": true
    }
  },
  "minimum-stability": "dev"
}
